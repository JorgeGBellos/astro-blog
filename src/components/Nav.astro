---
//library imports
import { Icon } from "astro-icon";
//data imports
import navData from "../data/navData";
//components imports
import Link from "./Link.astro";
---

<nav aria-label="Primary">
  <div class="navbar">
    <a href="/" class="logomark" aria-label="Go home">
      <Icon name="chess" width="40" />
    </a>
    <ul>
      {
        navData.map((element) => (
          <Link
            href={element.path}
            text={element.name}
            style="primary"
            isFilled={true}
            data-navLink
          />
        ))
      }
    </ul>
  </div>
</nav>

<script>
    import "../js/nav.js";
</script>